#. +> trunk5
#: ../../reference_manual/blending_modes/modulo.rst:37
#: ../../reference_manual/blending_modes/modulo.rst:42
msgid "Divisive Modulo - Continuous"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/modulo.rst:46
msgid "This blending mode is called \"Divisive Modulo - Continuous\" in English."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/modulo.rst:48
msgid "First, base layer is divided by the sum of the blend layer and the minimum possible value after zero. Then, performs a modulo calculation using the value found with the sum of the blend layer and the minimum possible value after zero. As this is a continuous mode, anything between odd to even numbers are inverted."
msgstr ""

